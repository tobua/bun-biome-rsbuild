{
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["DOM", "ES2020"],
		"module": "ESNext",
		"jsx": "react-jsx",
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"isolatedModules": true,
		"resolveJsonModule": true,
		"moduleResolution": "bundler",
		"types": ["@rsbuild/core/types", "bun"]
	},
	"include": ["index.tsx", "test"]
}
